{
  "name": "markocupic/calendar-event-booking-frontend-bundle",
  "description": "Contao calendar event booking frontend bundle.",
  "keywords": [
    "contao",
    "contao 4",
    "event booking",
    "bundle",
    "member list",
    "frontend modules",
    "remove the version property when downloading from packagist",
    "installation: download the package as a zip-file from github and upload it with contao manager"
  ],
  "version": "1.0.1",
  "type": "contao-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Marko Cupic",
      "email": "m.cupic@gmx.ch",
      "homepage": "https://github.com/markocupic",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/markocupic/calendar-event-booking-frontend-bundle/issues",
    "source": "https://github.com/markocupic/calendar-event-booking-frontend-bundle"
  },
  "require": {
    "contao/core-bundle": "^4.4",
    "markocupic/calendar-event-booking-bundle": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\CalendarEventBookingFrontendBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\CalendarEventBookingFrontendBundle\\ContaoManager\\Plugin"
  }
}
